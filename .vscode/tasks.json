{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "Run AVH",
			"command": "FVP_Corstone_SSE-300_Ethos-U55.exe",
			"args": [
				"-f",
				"Board\\AVH_MPS3_Corstone-300\\fvp_config.txt",
				"${command:cmsis-csolution.getBinaryFile}",
				"-Q",
				"10"
			],
			"problemMatcher": [],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"type": "cmsis-csolution.build",
			"project": "${command:cmsis-csolution.getCprjPath}",
			"problemMatcher": [],
			"label": "cmsis-csolution.build: Build"
		},
		{
			"type": "embedded-debug.flash",
			"serialNumber": "${command:device-manager.getSerialNumber}",
			"program": "${command:cmsis-csolution.getBinaryFile}",
			"cmsisPack": "${env:CMSIS_PACK_ROOT}/.Download/NXP.K32L3A60_DFP.13.0.0.pack",
			"problemMatcher": [],
			"label": "Flash K32L3A6",
			"group": "build"
		},
		{
			"type": "embedded-debug.flash",
			"serialNumber": "${command:device-manager.getSerialNumber}",
			"program": "${command:cmsis-csolution.getBinaryFile}",
			"cmsisPack": "${env:CMSIS_PACK_ROOT}/.Download/NXP.MIMXRT1052_DFP.15.0.0.pack",
			"problemMatcher": [],
			"label": "Flash MIMXRT1050",
			"group": "build"
		},
		{
			"type": "embedded-debug.flash",
			"serialNumber": "${command:device-manager.getSerialNumber}",
			"program": "${command:cmsis-csolution.getBinaryFile}",
			"cmsisPack": "${env:CMSIS_PACK_ROOT}/.Download/NXP.LPC55S69_DFP.15.0.0.pack",
			"problemMatcher": [],
			"label": "Flash LPC55S69",
			"group": "build"
		},
		{
			"type": "embedded-debug.flash",
			"serialNumber": "${command:device-manager.getSerialNumber}",
			"program": "${command:cmsis-csolution.getBinaryFile}",
			"cmsisPack": "${env:CMSIS_PACK_ROOT}/.Download/Keil.STM32G0xx_DFP.1.4.0.pack",
			"problemMatcher": [],
			"label": "Flash STM32G0B1",
			"group": "build"
		},
		{
			"type": "embedded-debug.flash",
			"serialNumber": "${command:device-manager.getSerialNumber}",
			"program": "${command:cmsis-csolution.getBinaryFile}",
			"cmsisPack": "${env:CMSIS_PACK_ROOT}/.Download/Keil.STM32G4xx_DFP.1.5.0.pack",
			"problemMatcher": [],
			"label": "Flash STM32G474",
			"group": "build"
		},
		{
			"type": "embedded-debug.flash",
			"serialNumber": "${command:device-manager.getSerialNumber}",
			"program": "${command:cmsis-csolution.getBinaryFile}",
			"cmsisPack": "${env:CMSIS_PACK_ROOT}/.Download/Keil.STM32L4xx_DFP.2.6.1.pack",
			"problemMatcher": [],
			"label": "Flash STM32L475",
			"group": "build"
		}
	]
}